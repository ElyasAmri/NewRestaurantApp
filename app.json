{
  "expo": {
    "name": "NewRestaurantApp",
    "slug": "new-restaurant-app",
    "version": "0.4.4",
    "orientation": "portrait",
    "icon": "./assets/expo/images/icon.png",
    "scheme": "nra",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/expo/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/expo/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 72
    },
    "web": {
      "favicon": "./assets/expo/images/favicon.png"
    },
    "extra": {
      "app_env": "local",
      "menu_fetch_url": "http://localhost:8000/api/menu"
    }
  }
}
